<div class="container">

    <% for (var i=0; i<books.length;i++) {%>

    <div class="row">

        <%for (var j=0;j<books[i].length;j++) {%>
        <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
                <a href="/books/<%= books[i][j]._id%>">
                    <img src=<%= books[i][j].coverImage%> alt="<%= books[i][j].title%>" class="img-responsive" >
                    <div class="caption">
                        <h3><%= books[i][j].title%></h3>
                        <h5><%= books[i][j].author %></h5>
                        <p class="description"><%= books[i][j].genre_tags%></p>
                        <div class="clearfix">
                            <div class="price pull-left">$<%= books[i][j].unitPrice%></div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
        <%}%>




    </div>
    <% } %>

</div>
